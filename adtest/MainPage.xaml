<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="adtest.MainPage">

    <StackLayout>
        <Frame BackgroundColor="#2196F3" Padding="24" CornerRadius="0">
            <Label Text="Welcome to Xamarin.Forms!" HorizontalTextAlignment="Center" TextColor="White" FontSize="36"/>
        </Frame>
        <Entry x:Name="entry1" Placeholder="Input1" HorizontalTextAlignment="Center" />
        <Entry x:Name="entry2" Placeholder="Input2" HorizontalTextAlignment="Center" />
        <!--<Button x:Name="submit" Text="BUTTON" Clicked="submit_Clicked" />-->
        <Label x:Name="result" Text="Result" HorizontalTextAlignment="Center"/>
    </StackLayout>

</ContentPage>
